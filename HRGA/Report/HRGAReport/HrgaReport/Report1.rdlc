      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     nrp, NAMA, Golongan, TINGKATAN, Dept, Absent, jkp_opr
FROM         QLembur1
WHERE     (Dept = @Dept) AND (tanggal BETWEEN @Awal AND @Akhir)
Order By Absent, Nrp</CommandText>
