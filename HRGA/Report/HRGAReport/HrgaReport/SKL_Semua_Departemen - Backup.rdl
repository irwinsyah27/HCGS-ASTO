  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT A.NoSKL, B.Departemen, A.Aprovedept, A.TglAproved, A.Aprovehr, A.TglHRAproved, CASE A.SKL WHEN 'RT' THEN 'REST TIME' WHEN 'SN' THEN 'SENAM PAGI' WHEN 'PK' THEN 'PIKET MALAM' WHEN 'H7' THEN 'HARI KE 7' WHEN 'N' THEN 'HARI BIASA' ELSE A.SKL END AS SKL, A.Status, A.NrpRequestor, A.NRPAproved, A.NRPHR FROM Tbl_SKL1 AS A INNER JOIN Persis.dbo.tblKaryawan AS B ON A.NrpRequestor = B.Nrp WHERE (B.Departemen IN (@DEPT))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DEPT">
            <Value>=Parameters!DEPT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DataSource2</DataSourceName>
      </Query>
