  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <DataSourceName>Departemen</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Site">
            <Value>=Parameters!Site.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Gol">
            <Value>=Parameters!Gol.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     QLembur2.tanggal, QLembur2.nrp, QLembur2.NAMA, QLembur2.shift, QLembur2.[in], QLembur2.out, QLembur2.NoMesin1, QLembur2.NoMesin2, 
                      tblLokasiMesin_1.LOKASI AS Lokasi1, tblLokasiMesin.LOKASI AS Lokasi2
FROM         QLembur2 LEFT OUTER JOIN
                      tblLokasiMesin ON QLembur2.NoMesin2 = tblLokasiMesin.ID LEFT OUTER JOIN
                      tblLokasiMesin AS tblLokasiMesin_1 ON QLembur2.NoMesin1 = tblLokasiMesin_1.ID INNER JOIN
                      Persis.dbo.tblKaryawan ON QLembur2.nrp = Persis.dbo.tblKaryawan.Nrp
WHERE     (QLembur2.tanggal BETWEEN @Awal AND @Awal) AND (QLembur2.Dept IN (@Dept)) AND (Persis.dbo.tblKaryawan.Company IN (@Company)) AND 
                      (Persis.dbo.tblKaryawan.Lokasi IN (@Site)) AND (Persis.dbo.tblKaryawan.Golongan BETWEEN (CASE WHEN @Gol = '19-UP' THEN 19 ELSE 0 END) AND 
                      (CASE WHEN @Gol = '19-UP' THEN 28 ELSE 18 END))</CommandText>
      </Query>
