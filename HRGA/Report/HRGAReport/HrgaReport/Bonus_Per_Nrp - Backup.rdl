    <DataSet Name="DataSource2">
      <Query>
        <CommandText>SELECT     nrp, NAMA, TINGKATAN, tanggal, [cn unit], Alat, jkp_opr, Bulan, Tahun, Tarif, Pencapaian, FatalInsiden
FROM         QBonus2
WHERE     (nrp IN (@Nrp)) AND (Bulan = @Bulan)</CommandText>
        <QueryParameters>
