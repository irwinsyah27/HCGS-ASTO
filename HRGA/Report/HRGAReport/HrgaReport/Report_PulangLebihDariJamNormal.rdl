    <DataSet Name="DS_Absen">
      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     lembarkerja.tanggal, lembarkerja.shift, PtblKaryawan.Nrp, PtblKaryawan.Golongan, PtblKaryawan.JABATAN, PtblKaryawan.Departemen, (SELECT COUNT(*) AS Total
                            FROM          Persis.dbo.tblKaryawan
                            WHERE      (Company = PtblKaryawan.Company) AND (Departemen = PtblKaryawan.Departemen)
                            GROUP BY Departemen) AS Total, 
                      PtblKaryawan.Lokasi, PtblKaryawan.Company, lembarkerja.Absent, CASE WHEN Golongan &gt;= 15 AND 
                      Golongan &lt;= 18 THEN 'GL' WHEN Golongan &lt; 15 THEN 'NonGL' END AS Posisi, CAST(lembarkerja.[in] AS time) AS Datang, CAST(lembarkerja.out AS time) AS Pulang, 
                      CASE WHEN shift = 1 AND datediff(hour, [in], [out]) &gt; 12 THEN 13 WHEN shift = 1 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) &lt; 13 THEN datediff(hour, 
                      [in], [out]) WHEN shift = 2 AND datediff(hour, [in], [out]) &gt; 12 THEN 13 WHEN shift = 2 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) 
                      &lt; 13 THEN datediff(hour, [in], [out]) END AS AsumsiLembur
FROM         Persis.dbo.tblKaryawan AS PtblKaryawan INNER JOIN
                      lembarkerja ON PtblKaryawan.Nrp = lembarkerja.nrp
WHERE     (PtblKaryawan.OnSite = 1) AND (PtblKaryawan.Company = @Company) AND (PtblKaryawan.Lokasi IS NOT NULL) AND (lembarkerja.tanggal BETWEEN @Awal AND @Akhir) AND (lembarkerja.[in] IS NOT NULL) AND 
                      (lembarkerja.out IS NOT NULL) AND (PtblKaryawan.Departemen IN (@Dept)) AND (DATEDIFF(hour, lembarkerja.[in], lembarkerja.out) &gt; 9) AND (PtblKaryawan.Golongan &lt;= 18)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
