  <DataSets>
    <DataSet Name="DS_Dept">
      <Query>
        <DataSourceName>DS_Persis</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Departemen, COUNT(*) AS Total
FROM         tblKaryawan
WHERE     (Company IN (@Company))
GROUP BY Departemen</CommandText>
      </Query>
      <Fields>
        <Field Name="Departemen">
          <DataField>Departemen</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
