          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Company">
      <Query>
        <DataSourceName>DS_Persis</DataSourceName>
        <CommandText>SELECT     Company
FROM         tblKaryawan
GROUP BY Company</CommandText>
      </Query>
      <Fields>
