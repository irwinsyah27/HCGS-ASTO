      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Tgl">
            <Value>=Parameters!Tgl.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AsumsiLembur">
            <Value>=Parameters!AsumsiLembur.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Posisi">
            <Value>=Parameters!Posisi.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Shift">
            <Value>=Parameters!Shift.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Lokasi">
            <Value>=Parameters!Lokasi.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     lembarkerja.id AS IdAbsen, lembarkerja.tanggal, lembarkerja.shift, PtblKaryawan.Nrp, PtblKaryawan.Golongan, PtblKaryawan.JABATAN, PtblKaryawan.Departemen AS Dept,
                           PtblKaryawan.Lokasi, PtblKaryawan.Company, lembarkerja.Absent, CASE WHEN Golongan &gt;= 15 AND 
                      Golongan &lt;= 18 THEN 'GL' WHEN Golongan &lt; 15 THEN 'NonGL' END AS Posisi, CAST(lembarkerja.[in] AS time) AS Datang, CAST(lembarkerja.out AS time) AS Pulang, 
                      CASE WHEN shift = 1 AND datediff(hour, [in], [out]) &gt; 12 THEN 13 WHEN shift = 1 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) &lt; 13 THEN datediff(hour, 
                      [in], [out]) WHEN shift = 2 AND datediff(hour, [in], [out]) &gt; 12 THEN 13 WHEN shift = 2 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) 
                      &lt; 13 THEN datediff(hour, [in], [out]) END AS AsumsiLembur, PtblKaryawan.Nama, lembarkerja.UserUpdate, lembarkerja.NoMesin2, lembarkerja.NoMesin1, 
                      lembarkerja.out, lembarkerja.[in], ISNULL
                          ((SELECT     LOKASI
                              FROM         tblLokasiMesin
                              WHERE     (ID = lembarkerja.NoMesin1)), N'-') AS Mesin1, ISNULL
                          ((SELECT     LOKASI
                              FROM         tblLokasiMesin AS tblLokasiMesin_1
                              WHERE     (ID = lembarkerja.NoMesin2)), N'-') AS Mesin2
FROM         Persis.dbo.tblKaryawan AS PtblKaryawan INNER JOIN
                      lembarkerja ON PtblKaryawan.Nrp = lembarkerja.nrp
WHERE     (PtblKaryawan.OnSite = 1) AND (PtblKaryawan.Company IN (@Company)) AND (PtblKaryawan.Lokasi IS NOT NULL) AND (lembarkerja.tanggal = @Tgl) AND (lembarkerja.[in] IS NOT NULL) AND (lembarkerja.out IS NOT NULL) AND (PtblKaryawan.Departemen IN (@Dept)) AND (DATEDIFF(hour, lembarkerja.[in], 
                      lembarkerja.out) &gt; 9) AND (PtblKaryawan.Golongan &lt;= 18) AND
(CASE WHEN shift = 1 AND datediff(hour, [in], [out]) 
                      &gt; 12 THEN 13 WHEN shift = 1 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) &lt; 13 THEN datediff(hour, [in], [out]) WHEN shift = 2 AND datediff(hour, [in], 
                      [out]) &gt; 12 THEN 13 WHEN shift = 2 AND datediff(hour, [in], [out]) &gt; 9 AND datediff(hour, [in], [out]) &lt; 13 THEN datediff(hour, [in], [out]) END) = @AsumsiLembur AND
CASE WHEN Golongan &gt;= 15 AND Golongan &lt;= 18 THEN 'GL' WHEN Golongan &lt; 15 THEN 'NonGL' END = @Posisi AND Shift=@Shift AND Lokasi = @Lokasi</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
