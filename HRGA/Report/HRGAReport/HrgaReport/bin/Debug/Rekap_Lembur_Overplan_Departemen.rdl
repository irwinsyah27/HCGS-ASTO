  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Departemen">
            <Value>=Parameters!Departemen.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@a">
            <Value>=Parameters!a.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@b">
            <Value>=Parameters!b.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Gol">
            <Value>=Parameters!Gol.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Site">
            <Value>=Parameters!Site.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
a.dept, 
count(nrp) as quanty ,
sum(a.plan_jk) as [plan],
Round(sum(a.lembur),2) as actual ,
SUM(a.amount_plan) as amount_plan,
SUM(a.amount_actual) as amount_actual,
SUM(a.amount_actual) - SUM(a.amount_plan) as amount_over

 from (

SELECT AA.*,
BB.Gaji,
ROUND((CONVERT(DECIMAL,BB.Gaji,1) / 173) * (CASE WHEN AA.Golongan &lt;= 18 THEN ROUND(AA.Plan_JK,1) ELSE 0 END ),2) AS amount_plan, 
ROUND((CONVERT(DECIMAL,BB.Gaji,1) / 173) * (CASE WHEN AA.Golongan &lt;= 18 THEN ROUND(AA.Lembur,1) ELSE 0 END ),2) AS amount_actual

FROM (

SELECT     nrp, 
		   NAMA, 
		   Dept, 
		   Golongan, 
		   SUM(Lembur) AS Lembur, 
		   SUM(MalamHari) AS MalamHari, 
		   SUM(SiangHari) AS SiangHari, 
		   SUM(TotalHari) AS TotalHari, 
		   SUM(Plan_JK) AS Plan_JK
FROM      (
				SELECT     
				A.nrp, 
				A.NAMA, 
				A.OprNonOpr, 
				A.Dept, 
				A.Bulan, 
				A.tanggal, 
				'15/' + CONVERT(Varchar, CAST(SUBSTRING(A.Bulan, 1, 2) AS Integer) - 1) 
                + '/' + SUBSTRING(A.Bulan, 4, 4) AS Awal, '16/' + SUBSTRING(A.Bulan, 1, 2) + '/' + SUBSTRING(A.Bulan, 4, 4) AS Akhir, ((((((A.Ke1 * 1.5 + A.Ke2 * 2) 
                                              + A.Ke17 * 2) + A.Ke8 * 3) + A.Ke9dst * 4) + A.L17 * 3) + A.L8 * 4) + A.L9dst * 5 AS Lembur, B.JkpXfaXfatalXfspXFpXfe AS Bonus, A.MalamHari, A.SiangHari, 
                                              A.MalamHari + A.SiangHari AS TotalHari, A.Golongan, 

CASE WHEN (SELECT Tanggal FROM Persis.dbo.tblHariLibur Where Tanggal = A.Tanggal) IS NULL                     
THEN CASE WHEN A.Plan_JK &gt; 1 THEN ((A.Plan_JK - 1) * 2) + 1.5 ELSE A.Plan_JK * 1.5 
END ELSE 
CASE WHEN A.Absent NOT IN ('I', 'J', 'Q', 'E') THEN
(7*2) + (A.Plan_JK * 3)
ELSE 0
END
END AS Plan_JK

                       FROM          DATA_ABS1.dbo.VW_Rekap_Lembur_tgl AS A LEFT OUTER JOIN
                                     DATA_ABS1.dbo.vw_Rekap_Bonus_a AS B ON A.nrp = B.Nrp AND A.Bulan = B.Bulan AND A.Bulan = B.Fpi_Bulan
                       WHERE      (A.Dept IN (@Departemen)) AND (A.tanggal BETWEEN @a AND @b)) AS TBL
Where Golongan BETWEEN (CASE When @Gol='19-UP' THEN 19 ELSE 0 END) AND (CASE WHEN @Gol='19-UP' THEN 28 ELSE 18 END)AND NRP IN (
Select NRP From Persis.dbo.tblKaryawan where Company IN (@Company) AND Departemen IN (@Departemen)AND Lokasi IN (@Site)
 )
GROUP BY nrp,NAMA,Dept,Golongan ) AS AA
INNER JOIN Persis.dbo.tblKaryawan AS BB
ON AA.nrp = BB.Nrp ) as a
WHERE a.Lembur &gt; a.Plan_JK
group by dept
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
