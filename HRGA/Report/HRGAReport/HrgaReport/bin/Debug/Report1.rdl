          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="HR">
          <DataField>HR</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="HRPGA">
          <DataField>HRPGA</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PM">
          <DataField>PM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <CommandText>Select Departemen, Keperluan, SUM(Tot) Tot, SUM(Atasan) AS Atasan , SUM(HR) AS HR, SUM(HRPGA) AS HRPGA, SUM(PM) AS PM
FROM(
SELECT Departemen, Keperluan, Tot, CASE WHEN Status='ATASAN' OR Status='Proses Dept.' THEN avgINTERVAL ELSE 0 END AS Atasan, CASE WHEN Status='Personel' OR Status='Proses HRGA' THEN avgINTERVAL ELSE 0 END AS HR, CASE WHEN Status='HRPGA Dept. Head' OR Status='HRPGA Sect. Head' THEN avgINTERVAL ELSE 0 END AS HRPGA, CASE WHEN Status='PM' THEN avgINTERVAL ELSE 0 END AS PM
FROM(
SELECT 
      [Departemen]
      ,[Keperluan]
      ,[Status]
      ,AVG([INTERVAL]) AS avgINTERVAL
      ,COUNT(NoID) AS Tot
  FROM [Persis].[dbo].[vw_MATRIX_SCSTSDIJINSKL]
  GROUP BY [Departemen]
      ,[Keperluan]
      ,[Status]) AS Tbl) AS TBLALL
      GROUP BY Departemen , Keperluan
      ORDER BY Departemen</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
