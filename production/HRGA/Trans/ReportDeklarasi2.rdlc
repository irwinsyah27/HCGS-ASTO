      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>
			SELECT     NRP, NAMA, DEPARTEMEN, REKENING, UANG_MUKA,NOMORST,ACCCODE, SUM(GAJI) AS GAJI, SUM(UDL) AS UDL, SUM(OBAT) AS OBAT, SUM(RS) AS RS,
			SUM(TRANSPORT) AS TRANSPORT, SUM(TIKET) AS TIKET, SUM(AKOMODASI) AS AKOMODASI, SUM(POTONGAN) AS POTONGAN, SUM(UPD)
			AS UPD
			FROM         VW_TRANSAKSI
			WHERE     (TGL BETWEEN @AWAL AND @AKHIR)AND NOMORST IS NOT NULL
			GROUP BY NRP, NAMA, DEPARTEMEN, REKENING, Uang_Muka,NOMORST,ACCCODE</CommandText>
        <DataSourceName>persisConnectionString</DataSourceName>
      </Query>
