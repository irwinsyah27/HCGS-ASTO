      <Query>
        <DataSourceName>DS_Persis</DataSourceName>
        <CommandText>SELECT     Lokasi, Company, Departemen, Keperluan, SUM(Tot) AS Tot, SUM(Atasan) AS Atasan, SUM(HR) AS HR, SUM(HRPGA) AS HRPGA, SUM(PM) AS PM, CASE WHEN Company = 'KPP' THEN 0 ELSE 1 END AS Urut
FROM         (SELECT     Lokasi, Company, Departemen, Keperluan, Tot, CASE WHEN Status = 'ATASAN' OR
                                              Status = 'Proses Dept.' THEN avgINTERVAL ELSE 0 END AS Atasan, CASE WHEN Status = 'Personel' OR
                                              Status = 'Proses HRGA' THEN avgINTERVAL ELSE 0 END AS HR, CASE WHEN Status = 'HRPGA Dept. Head' OR
                                              Status = 'HRPGA Sect. Head' THEN avgINTERVAL ELSE 0 END AS HRPGA, CASE WHEN Status = 'PM' THEN avgINTERVAL ELSE 0 END AS PM
                       FROM          (SELECT     Lokasi, Company, Departemen, Keperluan, Status, AVG(INTERVAL) AS avgINTERVAL, COUNT(NoID) AS Tot
                                               FROM          vw_MATRIX_SCSTSDIJINSKL
                                               GROUP BY Lokasi, Company, Departemen, Keperluan, Status) AS Tbl) AS TBLALL
WHERE     (Lokasi = 'KPBN')
GROUP BY Lokasi, Company, Departemen, Keperluan
ORDER BY Urut, Departemen</CommandText>
