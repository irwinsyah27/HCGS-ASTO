  <DataSets>
    <DataSet Name="DS_MP">
      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     dbo.lembarkerja.tanggal, dbo.lembarkerja.shift, PtblKaryawan.Nrp, PtblKaryawan.Golongan, PtblKaryawan.JABATAN, 
                      PtblKaryawan.Departemen, PDept.Total , PtblKaryawan.Lokasi, PtblKaryawan.Company, dbo.lembarkerja.Absent,
                      CASE WHEN Golongan &gt;= 15 AND Golongan &lt;= 18 THEN 'GL' WHEN Golongan &lt; 15 THEN 'NonGL' ELSE 'SectUP' END AS Posisi,
                      cast([in] as time) AS Datang, cast([out] as time) AS Pulang,
                      CASE WHEN shift=1 and cast([out] as time) &gt;= '16:30' and cast([out] as time) &lt;= '15:00' THEN '16.30 - 17.00'
						   WHEN shift=1 and cast([out] as time) &gt; '17:00' THEN '&gt; 17.00' 
						   WHEN shift=2 and cast([out] as time) &gt;= '04:30' and cast([out] as time) &lt;= '05:00' THEN '04.30 - 05.00'
						   WHEN shift=2 and cast([out] as time) &gt; '05:00' THEN '&gt; 05.00' 
						   END AS AsumsiLembur
FROM         Persis.dbo.tblKaryawan AS PtblKaryawan INNER JOIN
                     dbo.lembarkerja ON PtblKaryawan.Nrp = dbo.lembarkerja.nrp
INNER JOIN (Select Departemen, COUNT(*) AS Total from Persis.dbo.tblKaryawan where Company = @Company GROUP BY Departemen) AS PDept ON PtblKaryawan.Departemen = PDept.Departemen

WHERE     (PtblKaryawan.OnSite = 1) AND Lokasi IS NOT NULL AND Tanggal BETWEEN @Awal AND @Akhir AND
PtblKaryawan.Departemen IN (@Dept) AND
CASE WHEN shift=1 and cast([out] as time) &gt;= '16:30' and cast([out] as time) &lt;= '15:00' THEN '16.30 - 17.00'
						   WHEN shift=1 and cast([out] as time) &gt; '17:00' THEN '&gt; 17.00' 
						   WHEN shift=2 and cast([out] as time) &gt;= '04:30' and cast([out] as time) &lt;= '05:00' THEN '04.30 - 05.00'
						   WHEN shift=2 and cast([out] as time) &gt; '05:00' THEN '&gt; 05.00' 
						   END IS NOT NULL</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
