          <DataField>Keterangan</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Absent">
          <DataField>Absent</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Bulan">
          <DataField>Bulan</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT     QLembur2.nrp, QLembur2.NAMA, QLembur2.Dept, QLembur2.tanggal, AbsentCode.Keterangan, QLembur2.Absent, CAST(DATEPART(m, QLembur2.tanggal) AS Varchar) 
                      + '/' + CAST(DATEPART(yyyy, QLembur2.tanggal) AS Varchar) AS Bulan
FROM         QLembur2 INNER JOIN
                      AbsentCode ON QLembur2.Absent = AbsentCode.Code
WHERE     (QLembur2.shift IS NOT NULL) AND ((CAST(DATEPART(m, QLembur2.tanggal) AS Varchar) + '/' + CAST(DATEPART(yyyy, QLembur2.tanggal) AS Varchar)) IN (@Bulan)) 
                      AND (QLembur2.Dept IN (@Dept))
ORDER BY QLembur2.tanggal</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Bulan">
            <Value>=Parameters!Bulan.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Bulan">
      <Fields>
        <Field Name="Bulan">
          <DataField>Bulan</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tahun">
          <DataField>Tahun</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Bulan1">
          <DataField>Bulan1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>Select Distinct CAST(DATEPART(m, 
                      QLembur2.tanggal) AS Varchar) + '/' + CAST(DATEPART(yyyy, QLembur2.tanggal) AS Varchar) AS Bulan, Year(QLembur2.tanggal) As Tahun , Month(QLembur2.tanggal) As Bulan1
from QLembur2 Order By  Year(QLembur2.tanggal) Desc, Month(QLembur2.tanggal) Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
