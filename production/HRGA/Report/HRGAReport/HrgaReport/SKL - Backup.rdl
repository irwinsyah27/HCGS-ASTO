  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT A.NoSKL,TYPE = CASE B.skl WHEN 'RT' THEN 'RESTTIME' WHEN 'N' THEN 'NORMAL' WHEN 'H7' THEN 'HARIKE7'  WHEN 'PK' THEN 'PIKET' WHEN 'SN' THEN 'SENAM' END,A.NRP,C.Nama,C.Dept,A.AwalLembur,A.AkhirLembur,A.TotalHR,B.NrpAproved As Atasan ,CASE A.Reject WHEN 1 THEN 0 ELSE 1 END  As Aprove1, B.NrpHR As HR,B.UraianLembur , CASE A.Reject WHEN 1 THEN 0 ELSE 1 END As Aprove2
FROM Tbl_Subskl1 A Inner Join Tbl_Skl1 B ON A.NoSKL = B.NoSKL Inner Join Karyawan C ON A.Nrp = C.Nrp
WHERE A.Reject = 0 AND A.RejectHR = 0
AND A.AwalLembur Between @Awal And @Akhir  AND A.Nrp In(@Nrp)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Nrp">
            <Value>=Parameters!Nrp.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DataSource2</DataSourceName>
      </Query>
      <Fields>
        <Field Name="NoSKL">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NoSKL</DataField>
        </Field>
        <Field Name="TYPE">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>TYPE</DataField>
        </Field>
        <Field Name="NRP">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NRP</DataField>
        </Field>
        <Field Name="Nama">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Nama</DataField>
        </Field>
        <Field Name="Dept">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Dept</DataField>
        </Field>
