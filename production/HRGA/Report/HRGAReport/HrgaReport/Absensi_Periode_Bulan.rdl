  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <CommandText>SELECT     vw_lembarkerja.nrp, Karyawan.NAMA, vw_lembarkerja.tanggal, vw_lembarkerja.[in], vw_lembarkerja.out, vw_lembarkerja.Absent
FROM         vw_lembarkerja INNER JOIN
                      Karyawan ON vw_lembarkerja.nrp = Karyawan.Nrp
WHERE     (vw_lembarkerja.tanggal &gt;= @tglAwal) AND (vw_lembarkerja.tanggal &lt;= @tglAkhir)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@tglAwal">
            <Value>=Parameters!tglAwal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@tglAkhir">
            <Value>=Parameters!tglAkhir.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DataSource2</DataSourceName>
      </Query>
