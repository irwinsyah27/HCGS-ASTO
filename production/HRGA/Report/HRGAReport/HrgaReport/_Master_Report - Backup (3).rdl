      </Fields>
      <Query>
        <DataSourceName>DS_Persis</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>Select Lokasi, Company, Departemen, Keperluan, SUM(Tot) Tot, SUM(Atasan) AS Atasan , SUM(HR) AS HR, SUM(HRPGA) AS HRPGA, SUM(PM) AS PM
FROM(
SELECT Lokasi, Company,Departemen, Keperluan, Tot, CASE WHEN Status='ATASAN' OR Status='Proses Dept.' THEN avgINTERVAL ELSE 0 END AS Atasan, CASE WHEN Status='Personel' OR Status='Proses HRGA' THEN avgINTERVAL ELSE 0 END AS HR, CASE WHEN Status='HRPGA Dept. Head' OR Status='HRPGA Sect. Head' THEN avgINTERVAL ELSE 0 END AS HRPGA, CASE WHEN Status='PM' THEN avgINTERVAL ELSE 0 END AS PM
FROM(
SELECT Lokasi, Company,
      [Departemen]
      ,[Keperluan]
      ,[Status]
      ,AVG([INTERVAL]) AS avgINTERVAL
      ,COUNT(NoID) AS Tot
  FROM [Persis].[dbo].[vw_MATRIX_SCSTSDIJINSKL]
  GROUP BY Lokasi, Company, [Departemen]
      ,[Keperluan]
      ,[Status]) AS Tbl) AS TBLALL
      GROUP BY Lokasi, Company, Departemen , Keperluan
      ORDER BY Departemen</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
