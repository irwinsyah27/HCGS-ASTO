      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT        Karyawan_1.NAMA, vw_lembarkerja_1.tanggal, vw_lembarkerja_1.[in], vw_lembarkerja_1.out, vw_lembarkerja_1.Absent, Karyawan_1.Dept, 
                         vw_lembarkerja_1.NoMesin1, vw_lembarkerja_1.NoMesin2, Persis.dbo.tblKaryawan.JABATAN, vw_lembarkerja_1.nrp
FROM            vw_lembarkerja AS vw_lembarkerja_1 INNER JOIN
                         Karyawan AS Karyawan_1 ON vw_lembarkerja_1.nrp = Karyawan_1.Nrp INNER JOIN
                         Persis.dbo.tblKaryawan ON vw_lembarkerja_1.nrp = Persis.dbo.tblKaryawan.Nrp
WHERE        (vw_lembarkerja_1.tanggal &gt;= @tglAwal) AND (vw_lembarkerja_1.tanggal &lt;= @tglAkhir) AND (Karyawan_1.Dept IN (@Dept))
ORDER BY Persis.dbo.tblKaryawan.JABATAN, Karyawan_1.Dept</CommandText>
        <QueryParameters>
          <QueryParameter Name="@tglAwal">
            <Value>=Parameters!tglAwal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@tglAkhir">
            <Value>=Parameters!tglAkhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
