      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>Select CASE WHEN Golongan &gt;18 THEN '19UP' ELSE '18DW' END AS Gol,* 
From vw_ATR 
Where  (tanggal Between @Awal And @Akhir)  And Dept IN (@Dept)
Order By Nrp Asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
