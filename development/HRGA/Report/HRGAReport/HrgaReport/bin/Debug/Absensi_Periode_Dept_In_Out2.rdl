  <DataSets>
    <DataSet Name="DataSource2">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@tglAwal">
            <Value>=Parameters!tglAwal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@tglAkhir">
            <Value>=Parameters!tglAkhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Site">
            <Value>=Parameters!Site.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Gol">
            <Value>=Parameters!Gol.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Karyawan_1.NAMA, vw_lembarkerja_1.tanggal, vw_lembarkerja_1.[in], vw_lembarkerja_1.out, vw_lembarkerja_1.Absent, Karyawan_1.Dept, 
                      vw_lembarkerja_1.NoMesin1, vw_lembarkerja_1.NoMesin2, Persis.dbo.tblKaryawan.JABATAN, vw_lembarkerja_1.nrp, ISNULL
                          ((SELECT     LOKASI
                              FROM         tblLokasiMesin
                              WHERE     (ID = vw_lembarkerja_1.NoMesin1)), N'-') AS Mesin1, ISNULL
                          ((SELECT     LOKASI
                              FROM         tblLokasiMesin AS tblLokasiMesin_1
                              WHERE     (ID = vw_lembarkerja_1.NoMesin2)), N'-') AS Mesin2, vw_lembarkerja_1.UserUpdate
FROM         vw_lembarkerja AS vw_lembarkerja_1 INNER JOIN
                      Karyawan AS Karyawan_1 ON vw_lembarkerja_1.nrp = Karyawan_1.Nrp INNER JOIN
                      Persis.dbo.tblKaryawan ON vw_lembarkerja_1.nrp = Persis.dbo.tblKaryawan.Nrp
WHERE     (vw_lembarkerja_1.tanggal &gt;= @tglAwal) AND (vw_lembarkerja_1.tanggal &lt;= @tglAkhir) AND (Karyawan_1.Dept IN (@Dept)) AND 
                      (Persis.dbo.tblKaryawan.Company IN (@Company)) AND (Persis.dbo.tblKaryawan.Lokasi IN (@Site)) AND (Karyawan_1.Golongan BETWEEN 
                      (CASE WHEN @Gol = '19-UP' THEN 19 WHEN @Gol = '18-DW' THEN 0 ELSE 0 END) AND 
                      (CASE WHEN @Gol = '19-UP' THEN 28 WHEN @Gol = '18-DW' THEN 18 ELSE 28 END)) AND Persis.dbo.tblKaryawan.OnSite = 1
ORDER BY Persis.dbo.tblKaryawan.JABATAN, Karyawan_1.Dept</CommandText>
      </Query>
