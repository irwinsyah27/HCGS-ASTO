        <Field Name="Keterangan">
          <DataField>Keterangan</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     A.NomorST, A.Nrp, B.Nama, B.Departemen , A.Awal, A.Akhir,A.Keterangan
FROM         tblSutu A Inner Join tblKaryawan B ON A.Nrp = B.Nrp
WHERE     (Awal &lt;= @Tanggal) AND (Akhir &gt; @Tanggal) And Keperluan = 'CUTI' AND PROJECTMANAGER IS NOT NULL</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Tanggal">
            <Value>=Parameters!Tanggal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
