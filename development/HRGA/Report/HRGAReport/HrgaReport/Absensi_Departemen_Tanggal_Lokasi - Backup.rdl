      <Query>
        <DataSourceName>Departemen</DataSourceName>
        <CommandText>SELECT        QLembur2.tanggal, QLembur2.nrp, QLembur2.NAMA, QLembur2.shift, QLembur2.[in], QLembur2.out, QLembur2.NoMesin1, QLembur2.NoMesin2, 
                         tblLokasiMesin_1.LOKASI AS Lokasi1, tblLokasiMesin.LOKASI AS Lokasi2, QLembur2.Dept
FROM            QLembur2 LEFT OUTER JOIN
                         tblLokasiMesin ON QLembur2.NoMesin2 = tblLokasiMesin.ID LEFT OUTER JOIN
                         tblLokasiMesin AS tblLokasiMesin_1 ON QLembur2.NoMesin1 = tblLokasiMesin_1.ID
WHERE        (QLembur2.tanggal BETWEEN @Awal AND @Awal) AND (QLembur2.Dept IN (@Dept)) AND (tblLokasiMesin.LOKASI IN (@Lokasi)) OR
                         (QLembur2.tanggal BETWEEN @Awal AND @Awal) AND (QLembur2.Dept IN (@Dept)) AND (tblLokasiMesin_1.LOKASI IN (@Lokasi))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Lokasi">
            <Value>=Parameters!Lokasi.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
