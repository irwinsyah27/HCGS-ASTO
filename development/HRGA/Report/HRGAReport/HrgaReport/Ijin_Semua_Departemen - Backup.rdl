  <DataSets>
    <DataSet Name="DataSource1">
      <Query>
        <CommandText>SELECT     B.Departemen, COUNT(A.nrp) AS Jumlah, C.TotalKaryawan
FROM         DATA_ABS1.dbo.lembarkerja AS A INNER JOIN
                      tblKaryawan AS B ON A.nrp = B.Nrp INNER JOIN
                      vw_TotalKaryawan AS C ON B.Departemen = C.Departemen
WHERE     (CAST(CAST(MONTH(A.tanggal) AS Varchar) + '/' + CAST(DAY(A.tanggal) AS Varchar) + '/' + CAST(YEAR(A.tanggal) AS Varchar) AS Datetime) BETWEEN 
                      @Awal AND @Akhir) AND (A.Absent IN ('C', 'D'))
GROUP BY B.Departemen, C.TotalKaryawan
ORDER BY Jumlah DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DataSource1</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Departemen">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Departemen</DataField>
        </Field>
        <Field Name="Jumlah">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Jumlah</DataField>
        </Field>
        <Field Name="TotalKaryawan">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>TotalKaryawan</DataField>
        </Field>
      </Fields>
    </DataSet>
