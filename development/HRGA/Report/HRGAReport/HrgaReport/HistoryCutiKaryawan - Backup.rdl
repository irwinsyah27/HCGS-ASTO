        <Field Name="Departemen">
          <DataField>Departemen</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     Tabel.Nrp, SUM(Tabel.SisaCutiPeriode1) AS SisaCutiPeriode1, SUM(Tabel.SisaCutiPeriode2) AS SisaCutiPeriode2, SUM(Tabel.SisaCutiBesar) 
                      AS SisaCutiBesar, SUM(Tabel.TotalSisaCuti) AS TotalSisaCuti, SUM(Tabel.AmbilCutiTahunan) AS AmbilCutiTahunan, SUM(Tabel.AmbilCutiBesar) 
                      AS AmbilCutiBesar, SUM(Tabel.TotalAmbilCuti) AS TotalAmbilCuti, SUM(Tabel.PotonganCuti) AS PotonganCuti, tblKaryawan_1.Nama, 
                      tblKaryawan_1.JABATAN, tblKaryawan_1.Departemen
FROM         (SELECT     Nrp, '0' AS SisaCutiPeriode1, '0' AS SisaCutiPeriode2, '0' AS SisaCutiBesar, '0' AS TotalSisaCuti, ISNULL(SUM(C_Tahunan), 0) 
                                              AS AmbilCutiTahunan, ISNULL(SUM(C_Besar), 0) AS AmbilCutiBesar, ISNULL(SUM(C_Tahunan + C_Besar), 0) AS TotalAmbilCuti, 
                                              '0' AS PotonganCuti
                       FROM          dbo.tblSutu
                       WHERE     (dbo.tblSutu.Status = 'Complete') AND (dbo.tblSutu.tglST BETWEEN DATEPART(YEAR, GETDATE()) - 3 AND GETDATE())
                       GROUP BY Nrp
                       UNION ALL
                       SELECT     NRP, '0' AS SisaCutiPeriode1, '0' AS SisaCutiPeriode2, '0' AS SisaCutiBesar, '0' AS TotalSisaCuti, '0' AS AmbilCutiTahunan, 
                                             '0' AS AmbilCutiBesar, '0' AS TotalAmbilCuti, ISNULL(SUM(POTONG), 0) AS PotonganCuti
                       FROM         dbo.TblPemotonganCuti
                       GROUP BY NRP
                       UNION ALL
                       SELECT     Nrp, ISNULL(SisaCutiPeriode1, 0) AS SisaCutiPeriode1, ISNULL(SisaCutiPeriode2, 0) AS SisaCutiPeriode2, ISNULL(SisaCutiBesar, 0) 
                                             AS SisaCutiBesar, ISNULL(SisaCutiPeriode1 + SisaCutiPeriode2 + SisaCutiBesar, 0) AS TotalSisaCuti, '0' AS AmbilCutiTahunan, 
                                             '0' AS AmbilCutiBesar, '0' AS TotalAmbilCuti, '0' AS PotonganCuti
                       FROM         dbo.tblKaryawan) AS Tabel INNER JOIN
                      dbo.tblKaryawan AS tblKaryawan_1 ON Tabel.Nrp = tblKaryawan_1.Nrp
WHERE     (Tabel.Nrp &lt;&gt; '')
AND Departemen IN (@Dept)
GROUP BY Tabel.Nrp, tblKaryawan_1.Nama, tblKaryawan_1.JABATAN, tblKaryawan_1.Departemen</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
