        </Field>
        <Field Name="Lokasi2">
          <DataField>Lokasi2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <CommandText>SELECT        QLembur2.Dept, QLembur2.tanggal, QLembur2.nrp, QLembur2.NAMA, QLembur2.[in], QLembur2.out, QLembur2.Absent, CAST(DATEPART(m, QLembur2.tanggal) 
                         AS Varchar) + '/' + CAST(DATEPART(yyyy, QLembur2.tanggal) AS Varchar) AS Bulan, 
                         CASE AbsentCode.Code WHEN '-' THEN '' ELSE AbsentCode.Keterangan END AS Keterangan, tblLokasiMesin.LOKASI AS Lokasi1, 
                         tblLokasiMesin_1.LOKASI AS Lokasi2
FROM            AbsentCode RIGHT OUTER JOIN
                         tblLokasiMesin AS tblLokasiMesin_1 RIGHT OUTER JOIN
                         QLembur2 ON tblLokasiMesin_1.ID = QLembur2.NoMesin2 LEFT OUTER JOIN
                         tblLokasiMesin ON QLembur2.NoMesin1 = tblLokasiMesin.ID ON AbsentCode.Code = QLembur2.Absent
WHERE        (QLembur2.Dept = @Dept) AND (CAST(DATEPART(m, QLembur2.tanggal) AS Varchar) + '/' + CAST(DATEPART(yyyy, QLembur2.tanggal) AS Varchar) = @Bulan)
ORDER BY QLembur2.tanggal</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Bulan">
            <Value>=Parameters!Bulan.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
