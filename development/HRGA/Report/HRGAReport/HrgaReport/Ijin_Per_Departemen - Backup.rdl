  <DataSets>
    <DataSet Name="DataSource1">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select  A.Nrp, B.Nama, B.Departemen, Count(A.Nrp) As Ijin
From data_abs1.dbo.lembarkerja A Inner Join persis.dbo.tblKaryawan B 
On A.Nrp=B.Nrp 
Where Cast(Cast(Month(Tanggal)As Varchar)+'/'+Cast(Day(Tanggal)As Varchar)+'/'+Cast(Year(Tanggal)As Varchar)As Datetime) 
Between @Awal And @Akhir and A.Absent IN ('C','D') And B.Departemen In (@Departemen)
GROUP BY A.Nrp,B.Nama,B.Departemen
ORDER BY A.Nrp ASC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Awal">
            <Value>=Parameters!Awal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Akhir">
            <Value>=Parameters!Akhir.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Departemen">
            <Value>=Parameters!Departemen.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DataSource1</DataSourceName>
      </Query>
