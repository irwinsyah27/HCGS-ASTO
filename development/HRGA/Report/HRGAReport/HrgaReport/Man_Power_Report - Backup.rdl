  <DataSets>
    <DataSet Name="DataSource3">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     nrp, NAMA, Dept, tanggal, shift, [cn unit], Alat, jkp_opr, jk = jk1, Absent, CAST(DATEPART(m, tanggal) AS Varchar) + '/' + CAST(DATEPART(yyyy, tanggal) 
                      AS Varchar) AS Bulan
FROM         QLembur2
WHERE     (tanggal BETWEEN @Awal AND @Akhir)
AND DEPT =@DEPT</CommandText>
        <DataSourceName>DataSource2</DataSourceName>
      </Query>
