        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_ABS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     dbo.lembarkerja.tanggal, dbo.lembarkerja.shift, Persis.dbo.tblKaryawan.Nrp, Persis.dbo.tblKaryawan.Golongan, Persis.dbo.tblKaryawan.JABATAN, 
                      Persis.dbo.tblKaryawan.Departemen, Persis.dbo.tblKaryawan.Lokasi, Persis.dbo.tblKaryawan.Company, dbo.lembarkerja.Absent,
                      CASE WHEN Golongan &gt;= 15 AND Golongan &lt;= 18 THEN 'GL' WHEN Golongan &lt; 15 THEN 'NonGL' ELSE 'SectUP' END AS Posisi,
                      cast([in] as time) AS Datang, cast([out] as time) AS Pulang,
                      CASE WHEN shift=1 and cast([out] as time) &gt;= '16:30' and cast([out] as time) &gt;= '15:00' THEN '16.30 - 17.00'
						   WHEN shift=1 and cast([out] as time) &gt; '17:00' THEN '17.00 &gt;' 
						   WHEN shift=2 and cast([out] as time) &gt;= '04:30' and cast([out] as time) &gt;= '05:00' THEN '04.30 - 05.00'
						   WHEN shift=2 and cast([out] as time) &gt; '05:00' THEN '17.00 &gt;' 
						   END AS AsumsiLembur
FROM         dbo.lembarkerja INNER JOIN
                      Persis.dbo.tblKaryawan ON dbo.lembarkerja.nrp = Persis.dbo.tblKaryawan.Nrp
WHERE     (Persis.dbo.tblKaryawan.OnSite = 1)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
